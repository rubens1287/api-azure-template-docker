{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The Root Schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "additionalProperties": true,
  "required": [
    "id",
    "name",
    "username",
    "email",
    "address",
    "phone",
    "website",
    "company"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "description": "An explanation about the purpose of this instance.",
      "default": 0,
      "examples": [
        1.0
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": [
        "Leanne Graham"
      ]
    },
    "username": {
      "$id": "#/properties/username",
      "type": "string",
      "title": "The Username Schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": [
        "Bret"
      ]
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The Email Schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": [
        "Sincere@april.biz"
      ]
    },
    "address": {
      "$id": "#/properties/address",
      "type": "object",
      "title": "The Address Schema",
      "description": "An explanation about the purpose of this instance.",
      "default": {},
      "examples": [
        {
          "city": "Gwenborough",
          "zipcode": "92998-3874",
          "suite": "Apt. 556",
          "geo": {
            "lat": "-37.3159",
            "lng": "81.1496"
          },
          "street": "Kulas Light"
        }
      ],
      "additionalProperties": true,
      "required": [
        "street",
        "suite",
        "city",
        "zipcode",
        "geo"
      ],
      "properties": {
        "street": {
          "$id": "#/properties/address/properties/street",
          "type": "string",
          "title": "The Street Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "Kulas Light"
          ]
        },
        "suite": {
          "$id": "#/properties/address/properties/suite",
          "type": "string",
          "title": "The Suite Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "Apt. 556"
          ]
        },
        "city": {
          "$id": "#/properties/address/properties/city",
          "type": "string",
          "title": "The City Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "Gwenborough"
          ]
        },
        "zipcode": {
          "$id": "#/properties/address/properties/zipcode",
          "type": "string",
          "title": "The Zipcode Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "92998-3874"
          ]
        },
        "geo": {
          "$id": "#/properties/address/properties/geo",
          "type": "object",
          "title": "The Geo Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": {},
          "examples": [
            {
              "lat": "-37.3159",
              "lng": "81.1496"
            }
          ],
          "additionalProperties": true,
          "required": [
            "lat",
            "lng"
          ],
          "properties": {
            "lat": {
              "$id": "#/properties/address/properties/geo/properties/lat",
              "type": "string",
              "title": "The Lat Schema",
              "description": "An explanation about the purpose of this instance.",
              "default": "",
              "examples": [
                "-37.3159"
              ]
            },
            "lng": {
              "$id": "#/properties/address/properties/geo/properties/lng",
              "type": "string",
              "title": "The Lng Schema",
              "description": "An explanation about the purpose of this instance.",
              "default": "",
              "examples": [
                "81.1496"
              ]
            }
          }
        }
      }
    },
    "phone": {
      "$id": "#/properties/phone",
      "type": "string",
      "title": "The Phone Schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": [
        "1-770-736-8031 x56442"
      ]
    },
    "website": {
      "$id": "#/properties/website",
      "type": "string",
      "title": "The Website Schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": [
        "hildegard.org"
      ]
    },
    "company": {
      "$id": "#/properties/company",
      "type": "object",
      "title": "The Company Schema",
      "description": "An explanation about the purpose of this instance.",
      "default": {},
      "examples": [
        {
          "catchPhrase": "Multi-layered client-server neural-net",
          "bs": "harness real-time e-markets",
          "name": "Romaguera-Crona"
        }
      ],
      "additionalProperties": true,
      "required": [
        "name",
        "catchPhrase",
        "bs"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/company/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "Romaguera-Crona"
          ]
        },
        "catchPhrase": {
          "$id": "#/properties/company/properties/catchPhrase",
          "type": "string",
          "title": "The Catchphrase Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "Multi-layered client-server neural-net"
          ]
        },
        "bs": {
          "$id": "#/properties/company/properties/bs",
          "type": "string",
          "title": "The Bs Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            "harness real-time e-markets"
          ]
        }
      }
    }
  }
}